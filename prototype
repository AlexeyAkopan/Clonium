class Item:
  def __init__(self, k, access, i, j):
    self.k = k// count of points in cell
    self.acess = access// 
    self.i = i
    self.j = j
  
class Field:
  def __init__(self,length,wight):
    self.length = length
    self.wight = wight
    self.items = [([Item(0,1)]*length)]*wigth
    
AList = []
DList_1 = []
DList_2 = []
Main_DList = DList_1
t=1
 
def Add_count(i,j):
  Field.items[i][j].k += Field.items[i][j].access
  if (Field.items[i][j].k>3):
    Main_DList.append([i,j])
    
def Clear_Dis_List:
  for c in range(len(Main_DL)):
