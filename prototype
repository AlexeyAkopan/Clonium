def Clear_Add_List:
  for c in range(len(AList)):
    Add_count(Alist.pop(0))
    
def Disclosure(i,j):
  Field.items[i][j].k -= 4
  for c in[-1,1]:
    if (Field.items[i+c,j].access==1)
    Add_List.append([i+c,j])
    Add_List.append([i,j+c])
    Clear_Add_List()
    
 
